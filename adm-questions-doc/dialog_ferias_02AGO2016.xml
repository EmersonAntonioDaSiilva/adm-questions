		<folder label="Global"></folder>
		<folder label="Concepts"></folder>
    </flow>
    <constants>
        <var_folder name="Home">
            <var_folder name="General" type="CONST"/>
        </var_folder>
    </constants>
    <variables>
        <var_folder name="Home">
			<var name="NOME" type="TEXT"/>
			<var name="DATA_NASC" type="TEXT"/>
			<var name="DATA_ADM" type="TEXT"/>
        </var_folder>
    </variables>
    <settings>
